{
	"env": {
		"production": {
			"presets": [
				[
					"@babel/preset-env",
					{
						"loose": true,
						"modules": false
					}
				],
				"@babel/preset-react"
			],
			"plugins": [
				"@babel/plugin-proposal-class-properties",
				[
					"import",
					{
						"libraryName": "antd",
					    "libraryDirectory": "es",
					    "style": true
					}
				]
			]
		},
		"development": {
			"presets": [
				[
					"@babel/preset-env",
					{
						"loose": true,
						"modules": false
					}
				],
				"@babel/preset-react"
			],
			"plugins": [
				"@babel/plugin-proposal-class-properties",
				[
					"import",
					{
						"libraryName": "antd",
					    "libraryDirectory": "es",
					    "style": true
					}
				]
			]	
		}
	}
}
